<a class="return-button" href="/wishlist">Return</a>
<a class="return-button" href="/tvSeriess">Search All TV Series</a>

<h1 class="resource-title">Tv Series</h1>


<ul>
  <% user.tvSeriess.forEach((tvSeries) => { %>
    <li>
    <img src="<%= tvSeries.imageSRC %>" alt="<%= tvSeries.name %>">
    <a href="/tvSeriess/<%= tvSeries.id %>"><h4><%= tvSeries.name %></h4></a>
    <h4>Year: <%= tvSeries.year %></h4>
    <h4>Average Rating: <%= tvSeries.averageRating %></h4>
    </li>
    <form method="POST" action="/wishlists/tvSeriess/<%= tvSeries.id %>">
      <input type="hidden" name="_method" value="DELETE">
      <button class="btn" style="background-color: #eec3c4">Delete</button>
    </form>
  <% }) %>
</ul>
